<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="tr.com.lucidcode.model">

    <class name="MoneyControlScrips" table="moneycontrol_scrips">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="industry"/>
        <property name="url"/>
        <property name="bseId"  column="bse_id"/>
        <property name="nseId"  column="nse_id"/>
        <property name="sector"/>
        <property name="status"/>

    </class>

</hibernate-mapping>