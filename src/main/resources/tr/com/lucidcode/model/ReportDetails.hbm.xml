<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="tr.com.lucidcode.model">

    <class name="ReportDetails" table="report_details">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="reportId" column="report_id"/>
        <property name="reportKey" column="report_key"/>
        <property name="reportValue" column="report_value"/>

    </class>

</hibernate-mapping>